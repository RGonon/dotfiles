:filetype on 
:set backspace=indent,eol,start

:colorscheme default
:syntax on 
:set rnu 
:set number
:set ruler

set colorcolumn=80
set textwidth=80

:set ttymouse=sgr

:set expandtab
:set tabstop=4
:set showmatch
:set ignorecase 
:set cursorline

:inoremap jj <Esc>
:autocmd FileType make set noexpandtab shiftwidth=8 softtabstop=0
:set clipboard=unnamedplus
